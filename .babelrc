{
    "presets": [
        [
            "@babel/env",
            {
                "modules": false,
                "targets": {
                    "browsers": [
                        "ios >= 7.1",
                        "Android >= 4.1"
                    ]
                }
            }
        ]
    ],
    "plugins": [
        [
            "@babel/transform-runtime",
            {
                "corejs": false,
                "helpers": false,
                "regenerator": false,
                "useESModules": false
            }
        ],
        "@babel/proposal-class-properties"
    ]
}